{block:tumblrThemr}
  <link rel="stylesheet" href="/stylesheets/site.css">
  <style type="text/css">{CustomCSS}</style>


  <script="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js"></script>
  {block:Description}
  <meta name="description" content="{MetaDescription}" />
  {/block:Description}

  <%= partial "header" %>

  <div class="row">
    <ol id="posts">
      {block:Posts}

      <%= partial "posts/text" %>
      <%= partial "posts/photo" %>
      <%= partial "posts/photoset" %>
      <%= partial "posts/quote" %>
      <%= partial "posts/link" %>
      <%= partial "posts/chat" %>
      <%= partial "posts/video"  %>
      <%= partial "posts/audio" %>

      {/block:Posts}
    </ol>
  </div>

  <%= partial "footer" %>

{/block:tumblrThemr}